<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">
    <f:entry title="Application Name" field="applicationId">
        <f:select/>
    </f:entry>
    <f:entry title="Release Name" field="releaseId">
        <f:select/>
    </f:entry>
    <f:entry title="Assessment Type" field="assessmentTypeId">
        <f:select/>
    </f:entry>
    <f:entry title="Entitlement Id" field="entitlementId">
        <f:select/>
    </f:entry>
    <f:entry title="Technology Stack" field="technologyStack">
        <f:select/>
    </f:entry>
    <f:entry title="Language Level" field="languageLevel">
        <f:select/>
    </f:entry>

    <f:advanced title="Additional Options">
        <f:entry title="Open source component analysis" field="runOpenSourceAnalysis">
            <f:checkbox/>
        </f:entry>
        <f:entry title="Enable express scan preference" field="isExpressScan">
            <f:checkbox/>
        </f:entry>
        <f:entry title="Enable automated audit preference" field="isExpressAudit">
            <f:checkbox/>
        </f:entry>
        <f:entry title="Enable detailed assessment result log" field="doPrettyLogOutput">
            <f:checkbox/>
        </f:entry>
        <f:entry title="Polling Interval" description="In minutes" field="pollingInterval">
            <f:textbox default="0"/>
        </f:entry>
        <f:entry title="Include all project files" field="includeAllFiles">
            <f:checkbox/>
        </f:entry>
        <f:entry title="Exclude third-party libraries" field="excludeThirdParty">
            <f:checkbox/>
        </f:entry>
        <f:entry title="Is a remediation scan" field="isRemediationScan">
            <f:checkbox/>
        </f:entry>
    </f:advanced>
</j:jelly>
